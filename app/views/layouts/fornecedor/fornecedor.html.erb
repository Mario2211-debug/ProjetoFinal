<!DOCTYPE html>
<html>
  <head>
    <title>ReclameAi</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
     <%= stylesheet_link_tag 'fornecedor/fornecedor', media: 'all', 'data-turbolinks-track': 'reload' %>
     <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
     <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
  </head>
  <body>
    <div class="main_content">
    <%=render 'layouts/fornecedor/sidebar'%>
    <%=render 'layouts/fornecedor/navbar'%>
    <%=render 'layouts/fornecedor/header'%>
    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, id: "flash_#{name}" %>
    <% end %>
    <%= yield %>
  </div>

  </body>
  <%= debug(params) if Rails.env.development? %>
</html>
