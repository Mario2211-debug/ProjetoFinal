
  <div class="main_content_inner">
    <div class="col-xl-4 col-lg-5">
      <div class="section-small">
        <div class="card">
          <div class="card-header">
  <h4 class="card-title">Criar um novo serviço</h4>
          </div>
        <div class="card-body">
          <div class="basic-form">

    <div class="row ">
                    <%= form_for @assunto,:url => {:action => :create}  do |f| %>
                    <div class="form-group">
                        <%= f.label :Nome_do_serviço_que_deseja_adicionar, class:'uk-form-label'%>
                        <%= f.text_field :nome, autofocus: true, autocomplete:  "Valor", placeholder:'Nome do serviço',  class:'uk-input', style:'background-color: #def0f8;' %>
                        </div>

                                                        <div class="text-right">
                                                          <%=f.submit 'Criar', class:'btn badge-success'%>
                                                          </div>
                      <% end %>
    </div>

    </div>

  </div>
    </div>
    </div>
  </div>
</div>
